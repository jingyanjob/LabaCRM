<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>	
	<class name="com.CRM.data.SalesCaseMonthRep" table="scmomrep">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="buid" column="buid" />
		<property name="sourcesite" column="sourcesite" />
		<property name="yyyy" column="yyyy" />
		<property name="mm" column="mm" />
		<property name="scnumber" column="scn" />
		<property name="vipnumber" column="vipn" />
		<property name="updatetime" column="updatetime" />
		<property name="inactnumber" column="inactnu" />
		<property name="dealnumber" column="dealn" />
		
	</class>
</hibernate-mapping>