<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.CRM.data.Reservation" table="reservation">
		<id name="id" column="id"/>
		<property name="uid" column="uid" />
		<property name="cid" column="cid" />
		<property name="buid" column="buid" />
		<property name="username" column="username" />
		<property name="createdat" column="createdat" />
		<property name="text" column="text" />
		<property name="resstatus" column="resstatus" />
		<property name="resresp" column="resresp" />
		<property name="updatetime" column="updatetime" />
	</class>
</hibernate-mapping>