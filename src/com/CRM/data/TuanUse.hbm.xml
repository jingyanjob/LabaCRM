<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.CRM.data.TuanUse" table="tuanuse">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="buid" column="buid" />
		<property name="tuancode" column="tuancode" />
		<property name="tuanuser" column="tuanuser" />
		<property name="updatetime" column="updatetime" />
	</class>
</hibernate-mapping>