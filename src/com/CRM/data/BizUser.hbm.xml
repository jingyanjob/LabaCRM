<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.CRM.data.BizUser" table="bizuser">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="uid" column="uid" />
		<property name="username" column="username" />
		<property name="sourcesite" column="sourcesite" />
		<property name="rtoken" column="rtoken" />
		<property name="rtokensecret" column="rtokensecret" />
		<property name="expirein" column="expirein" />
		<property name="atoken" column="atoken" />
		<property name="atokensecret" column="atokensecret" />
		<property name="repsinceid" column="repsinceid" />
		<property name="cmssinceid" column="cmssinceid" />
		<property name="cmsamsinceid" column="cmsamsinceid" />
		<property name="updatetime" column="updatetime" />
	</class>
</hibernate-mapping>